    y:=0.2
    z:=0.05
    Y:=0
    frame:=daihen_ur5::container"/>
    <arg name="node_name" value="spawn_onigiri_3"/>
  </include>
  <include file="$(find timed_roslaunch)/launch/timed_roslaunch.launch">
    <arg name="time" value="3" />
    <arg name="pkg" value="fcsc_description" />
    <arg name="file" value="spawn_objects_urdf.launch" />
    <arg name="value" value="
    xacro:=true
    use_ar_marker:=true
    marker_id:=4
    model:=$(find fcsc_description)/model/onigiri.urdf.xacro
    name:=onigiri_4
    x:=-0.15
    y:=0.2
    z:=0.05
    Y:=0
    frame:=daihen_ur5::container"/>
    <arg name="node_name" value="spawn_onigiri_4"/>
  </include>
</launch>
